    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "20.5.6",
    "openapi-typescript-codegen": "0.25.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.1",
    "tslib": "2.6.2"
